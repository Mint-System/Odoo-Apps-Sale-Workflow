<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="view_blanket_order_form_contact_person" model="ir.ui.view">
    <field name="name">view_blanket_order_form_contact_person</field>
    <field name="model">sale.blanket.order</field>
    <field name="inherit_id" ref="sale_blanket_order.view_blanket_order_form" />
    <field name="arch" type="xml">
      <xpath expr="//field[@name='partner_id']" position="after">
        <field name="partner_contact_id"/>
      </xpath>
    </field>
  </record>

</odoo>
